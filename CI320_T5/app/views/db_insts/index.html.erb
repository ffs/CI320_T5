<h1>Listing db_insts</h1>

<table>
  <thead>
    <tr>
      <th>Db inst name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @db_insts.each do |db_inst| %>
      <tr>
        <td><%= db_inst.db_inst_name %></td>
        <td><%= link_to 'Show', db_inst %></td>
        <td><%= link_to 'Edit', edit_db_inst_path(db_inst) %></td>
        <td><%= link_to 'Destroy', db_inst, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Db inst', new_db_inst_path %>
