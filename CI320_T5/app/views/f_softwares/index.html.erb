<h1>Listing f_softwares</h1>

<table>
  <thead>
    <tr>
      <th>F software name</th>
      <th>Cpu</th>
      <th>Memory</th>
      <th>Storage</th>
      <th>Version</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @f_softwares.each do |f_software| %>
      <tr>
        <td><%= f_software.f_software_name %></td>
        <td><%= f_software.cpu %></td>
        <td><%= f_software.memory %></td>
        <td><%= f_software.storage %></td>
        <td><%= f_software.version %></td>
        <td><%= link_to 'Show', f_software %></td>
        <td><%= link_to 'Edit', edit_f_software_path(f_software) %></td>
        <td><%= link_to 'Destroy', f_software, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New F software', new_f_software_path %>
